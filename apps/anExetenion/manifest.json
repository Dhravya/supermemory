{
  "manifest_version": 3,
  "version": "1.0",
  "name": "Supermemory.ai-Extension",
  "description": "Uses the chrome.contextMenus API to customize the context menu. To learn more checkout supermemory.ai",
  "background": {
    "service_worker": "./background.ts"
  },
  "permissions": [
    "contextMenus"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./content/content.tsx"
      ]
    }
  ],
  "icons": {
    "16": "public/icon/logo(16).png",
    "48": "public/icon/logo(48).png"
  }
}